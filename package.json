{
  "name": "react-booter",
  "version": "1.0.20",
  "description": "a booter for react",
  "main": "index.js",
  "bin": {
    "react-booter": "./bin/react-booter"
  },
  "files": [
    "bin",
    "dist",
    "config",
    "template",
    "config.js",
    "theme.less"
  ],
  "scripts": {
    "compileReact": "babel src/entry -d dist/entry --copy-files --config-file ./config/babel.config.js",
    "compileNode": "babel src/main -d dist/main --copy-files --config-file ./config/babel.config.node.js",
    "precompile": "rimraf ./dist/*",
    "compile": "npm run compileNode && npm run compileReact",
    "start": "node ./bin/react-booter-start",
    "prepublish": "npm run compile"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.0",
    "commander": "^2.19.0",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.2",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.7.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-less-theme-plugin": "^2.1.0",
    "webpack-merge": "^4.1.4"
  }
}
